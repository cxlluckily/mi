<?xml version="1.0" encoding="utf-8" ?>
<!--with 必须设置整数，对应Excel中的列宽，如果列宽是小数的话，建议四射五入-->
<settings>

    <setting ID="qrCodeListExport">
        <title>二维码导出</title>
        <description>二维码导出数据</description>
        <sheets>
            <sheet name="二维码数据" totalKey="isTotal" totalColSpan="5">
                <headers>
                    <header>
                        <column key="qrCode" display="二维码号码" width="30"/>
                        <column key="categoryName" display="备件类型" width="20"/>
                        <column key="partName" display="备件名称" width="30"/>
                        <column key="materiaCoding" display="物资编码" width="30"/>
                        <column key="specificationModel" display="规格型号" width="30"/>
                        <column key="statusText" display="状态" width="30"/>

                    </header>
                </headers>
            </sheet>
        </sheets>
    </setting>

    <setting ID="operationsListExport">
        <title>线上设备导出</title>
        <description>线上设备导出数据</description>
        <sheets>
            <sheet name="线上设备数据" totalKey="isTotal" totalColSpan="9">
                <headers>
                    <header>
                        <column key="lineName" display="线路" width="20"/>
                        <column key="stationName" display="站点" width="30"/>
                        <column key="categoryName" display="设备类型" width="30"/>
                        <column key="partName" display="备件名称" width="30"/>
                        <column key="equipmentName" display="设备名称" width="30"/>
                        <column key="equipmentNo" display="设备编号" width="30"/>
                        <column key="statusText" display="状态" width="30"/>
                        <column key="location" display="所在位置" width="20"/>
                        <column key="brand" display="品牌" width="30"/>
                        <column key="specificationModel" display="规格型号" width="30"/>

                    </header>
                </headers>
            </sheet>
        </sheets>
    </setting>

    <setting ID="shelfListExport">
