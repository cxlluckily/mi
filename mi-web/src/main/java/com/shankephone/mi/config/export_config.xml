    <!-- 多sheet示例 -->
    <setting ID="availableStockExport">
        <title>可用库存</title>
        <description>可用库存设置</description>
        <sheets>
            <sheet name="可用库存" totalKey="isTotal" totalColSpan="5">
                <headers>
                    <header>
                        <column key="factoryName" display="工厂名称" width="30"/>
                        <column key="warehouseName" display="仓库名称" width="20"/>
                        <column key="productNumber" display="产品编号" width="30"/>
                        <column key="productName" display="产品名称" width="20"/>
                        <column key="primaryUnitNumberInStock" display="实时库存" width="20"/>
                        <column key="primaryUnitNumberFrozen" display="昨日速冻报数" width="20"/>
                        <column key="primaryUnitNumberYesterday" display="昨日库存" width="20"/>
                        <column key="primaryUnitNumberCanBeSell" display="今日可供销售库存" width="20"/>
                        <column key="primaryUnitNumberIsSendOutToday" display="当日发货总数" width="20"/>
                        <column key="primaryUnitNumberIsSendOutAuditing" display="未审核发货单数量" width="20"/>
                        <column key="primaryUnitNumberIsAllocationOut" display="当日调拨单出库总数" width="20"/>
                        <column key="primaryUnitNumberIsAllocationIn" display="当日调拨单入库总数" width="20"/>
                        <column key="primaryUnitNumberOtherOut" display="当日其他库存操作出库出库总数" width="30"/>
                        <column key="primaryUnitNumberOtherIn" display="当日其他库存操作出库入库总数" width="30"/>
                        <column key="primaryUnit" display="主单位" width="20"/>
                        <column key="auxiliaryUnit" display="辅单位" width="20"/>
                        <column key="unitConvert" display="主辅单位换算" width="20"/>
                        <column key="categoryName" display="产品分类" width="20"/>
                        <column key="brandName" display="品牌" width="20"/>
                    </header>
                </headers>
            </sheet>
            <sheet name="销售情况">
                <headers>
                    <header>
                        <column key="factoryName" display="工厂名称" width="30"/>
                        <column key="warehouseName" display="仓库名称" width="20"/>
                        <column key="customerName" display="客户名称" width="20"/>
                        <column key="customerName" display="客户分类" width="20"/>
                        <column key="customerName" display="客户地区" width="20"/>
                        <column key="customerName" display="实际发货数量" width="20"/>
                        <column key="customerName" display="手机号" width="20"/>
                        <column key="productNumber" display="产品编号" width="30"/>
                        <column key="productName" display="产品名称" width="20"/>
                        <column key="primaryUnitNumber" display="订单主单位数量" width="20"/>
                        <column key="primaryUnit" display="主单位" width="20"/>
                        <column key="auxiliaryUnitNumber" display="订单辅单位数量" width="20"/>
                        <column key="auxiliaryUnit" display="辅单位" width="20"/>
                        <column key="unitConvert" display="主辅单位换算" width="20"/>
                        <column key="categoryName" display="产品分类" width="20"/>
                        <column key="brandName" display="品牌" width="20"/>
                        <column key="spec" display="规格" width="20"/>
                        <column key="costPrice" display="成本价(元)" width="20"/>
                        <column key="remark" display="产品备注" width="100"/>
                    </header>
                </headers>
            </sheet>
        </sheets>
    </setting>


    <!-- 多表头 多页签示例 -->
    <setting ID="exportScoreSheet">
        <title>成绩表</title>
        <description>导出成绩表</description>
        <sheets>
            <sheet name="E" bankValue="">
                <headers>
                    <header>
                        <column key="personNo" display="编号" width="6" rowSpan="2"></column>
                        <column key="chineseName" display="姓名" width="16" rowSpan="2"></column>
                        <column display="三阶（单位：秒）" colSpan="3"></column>
                        <column display="获得段位" width="10" rowSpan="2" defaultValue="E_"></column>
                        <column display="考生签字" width="14" rowSpan="2"></column>
                    </header>
                    <header>
                        <column display="成绩1" width="11" skip="2"></column>
                        <column display="成绩2" width="11"></column>
                        <column display="最终成绩" width="11"></column>
                    </header>
                </headers>
            </sheet>
            <sheet name="D" bankValue="">
